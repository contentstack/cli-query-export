      "pluginType": "core",
      "strict": true,
      "usage": "cm:stacks:export [-c <value>] [-k <value>] [-d <value>] [-a <value>] [--module <value>] [--content-types <value>] [--branch <value>] [--secured-assets]",
      "isESM": false,
      "relativePath": [
        "lib",
        "commands",
        "cm",
        "stacks",
        "export.js"
      ]
